{
    "name": "@chizaruu/supabase-type-generator",
    "version": "0.1.0",
    "description": "Complete Supabase Type Generator - Schema-File First with Index Support",
    "keywords": [
        "supabase",
        "typescript",
        "types",
        "generator",
        "schema",
        "postgresql",
        "sql"
    ],
    "author": "chizaruu",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/chizaruu/supabase-type-generator.git"
    },
    "bugs": {
        "url": "https://github.com/chizaruu/supabase-type-generator/issues"
    },
    "homepage": "https://github.com/chizaruu/supabase-type-generator#readme",
    "type": "module",
    "main": "./dist/index.ts",
    "types": "./dist/index.d.ts",
    "bin": {
        "generate-supabase-types": "./dist/bin/generate-types.ts"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "prepublishOnly": "npm run build",
        "type-check": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "knip": "npm knip"
    },
    "dependencies": {
        "glob": "^10.3.10"
    },
    "devDependencies": {
        "@types/node": "^20.19.27",
        "@vitest/coverage-v8": "^1.0.0",
        "knip": "^5.73.4",
        "typescript": "^5.9.3",
        "vitest": "^1.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
